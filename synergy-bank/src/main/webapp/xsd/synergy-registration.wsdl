<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://www.bank.synergy.com/registration/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="synergy-registration"
	targetNamespace="http://www.bank.synergy.com/registration/">
	<wsdl:types>
		<xsd:schema>
		   <xsd:import namespace="http://www.bank.synergy.com/registration/" schemaLocation="registrationForm.xsd"/>	
		</xsd:schema>
	</wsdl:types>
	
	<wsdl:message name="RegistrationInput">
	    <wsdl:part name="registrationInput" element="tns:registrationVOInput"/>
	</wsdl:message>
    
    <!-- 
       void kill(EmailMessageResponse remailMessageResponse){
       
       }
     -->
	<wsdl:message name="RegistrationResponse">
	 	<wsdl:part name="registrationResponse" element="tns:registrationVOOutput"/>
	</wsdl:message>
	
	<wsdl:portType name="SynergyRegistrationService">
	    <wsdl:operation name="addCustomer">
	       <wsdl:input message="tns:RegistrationInput"/>
	       <wsdl:output message="tns:RegistrationResponse"/>
	    </wsdl:operation>
	</wsdl:portType>
	
	<wsdl:binding name="SynergyRegistrationServiceBinding" type="tns:SynergyRegistrationService">
	   <soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http"/>
			<wsdl:operation name="addCustomer">
				<soap:operation soapAction="http://www.bank.synergy.com/registration/addCustomer" />
				 <wsdl:input>
				 	<soap:body use="literal"/>
				 </wsdl:input>			
				  <wsdl:output>
				  	<soap:body use="literal"/>
				  </wsdl:output> 
			</wsdl:operation>
	</wsdl:binding>
	
	 <wsdl:service name="SynergyRegistrationServiceProvider">
	     <wsdl:port name="SynergyRegistrationServicePort" binding="tns:SynergyRegistrationServiceBinding">
			  <soap:address location="http://localhost:8889/synergy-bank/ebank/registrationWebService"/>		     	     
	     </wsdl:port>
	 </wsdl:service>	

</wsdl:definitions>
