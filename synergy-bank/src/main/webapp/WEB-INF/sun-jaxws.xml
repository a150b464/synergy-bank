<?xml version="1.0" encoding="UTF-8"?>
<endpoints
  xmlns="http://java.sun.com/xml/ns/jax-ws/ri/runtime"
  version="2.0">
  <!-- Here we are registering web service 
  our web service can be accessed by below url-pattern

  we are registering here our soap web service......................
  ..............................................................
   -->
  <endpoint
      name="bankGallerySoapWebService"
      implementation="com.synergy.bank.soap.web.provider.BankGallerySoapWebService"
      url-pattern="/bankGallerySoapWebService"/>
     
  <endpoint
      name="SynerBankEmailProvider"
      implementation="com.synergy.bank.email.provider.SynerBankEmailProvider"
      url-pattern="/ebank/emailWebService"/>
      
  <endpoint
      name="SynerBankPayeeProvider"
      implementation="com.synergy.bank.payee.provider.SynergyBankPayeeProvider"
      url-pattern="/ebank/payeeWebService"/>
      
  <endpoint
      name="SynerBankRegistrationProvider"
      implementation="com.synergy.bank.registration.provider.SynergyBankRegistrationProvider"
      url-pattern="/ebank/registrationWebService"/>
      
  <endpoint
      name="bankPayeeSoapWebService"
      implementation="com.synergy.bank.soap.web.provider.BankPayeeSoapWebService"
      url-pattern="/bankPayeeSoapWebService"/>
      
</endpoints>